{
  "name": "nodejs-template",
  "version": "1.0.0",
  "private": true,
  "author": "ABRADOR Daryl",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "sass": "sass --watch ./public/scss/base/style.scss:./public/css/style.css",
    "compressed": "sass ./public/scss/base/style.scss:./public/css/style.css --style compressed",
    "doc": "./node_modules/.bin/jsdoc -c ./js-doc-conf.json",
    "test": "mocha --timeout 10000",
    "nosql": "npm install --save mongoose connect-mongodb-session csurf && npm install --save-dev mongodb",
    "sql": "npm install --save mysql2 sequelize"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.0.0",
    "http-errors": "~1.6.3",
    "jsdoc": "^3.6.5",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "sinon": "^9.0.3"
  }
}
